{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/tsdoc/v0/tsdoc.schema.json",
  "extends": ["typedoc/tsdoc.json"],
  "noStandardTags": false,
  "tagDefinitions": [
    {
      "tagName": "@customElement",
      "syntaxKind": "block"
    },
    {
      "tagName": "@attribute",
      "syntaxKind": "block",
      "allowMultiple": true
    },
    {
      "tagName": "@slot",
      "syntaxKind": "block",
      "allowMultiple": true
    }
  ]
}
